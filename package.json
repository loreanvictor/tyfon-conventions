{
  "name": "tyfon-conventions",
  "version": "0.1.1",
  "description": "TyFON conventions for mapping functions to endpoints",
  "main": "dist/es5/index.js",
  "module": "dist/es6/index.js",
  "types": "dist/es6/index.d.ts",
  "files": [
    "dist/es5",
    "dist/es6"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "ts-node --project conf/typescript/test.json test.ts",
    "build-es5": "tsc -p conf/typescript/es5.json",
    "build-es6": "tsc -p conf/typescript/es6.json",
    "build": "npm run build-es5 && npm run build-es6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loreanvictor/tyfon-conventions.git"
  },
  "author": "Eugene Ghanizadeh Khoub <ghanizadeh.eugene@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loreanvictor/tyfon-conventions/issues"
  },
  "homepage": "https://github.com/loreanvictor/tyfon-conventions#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
